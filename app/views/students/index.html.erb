<table class="studentstable">
  <caption> Code Academy Winter 2012 Students</caption>
  <th>Picture</th>
  <th>Name</th>
  <th>Class Day</th>
  <th>Class Type</th>
  <th>Gender</th>
<% @students.each do |student| %>
  <tr>
    <td><%= image_tag(student.image_url,
          :title => "#{student.first_name} #{student.last_name}") %>
    <td><%= student.first_name %> <%= student.last_name %></td>
    <td><%= student.class_day %></td>
    <td><%= student.class_type %></td>
    <td><%= student.gender %></td>
  </tr>
<% end %>
</table>
